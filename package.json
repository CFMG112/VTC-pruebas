{
  "name": "vcm",
  "description": "VCM Sentry Descktop",
  "author": "leading-technologies",
  "version": "1.0.1",
  "private": true,
  "main": "./dist-electron/main.js",
  "build": {
    "appId": "com.leadingtechnologies.vtc.sentry.desktop",
    "productName": "VCM Sentry Desktop",
    "copyright": "Copyright Â© 2020 Leading Technologies",
    "npmRebuild": "false",
    "extraFiles": [
      "config/**/*"
    ]
  },
  "scripts": {
    "ng": "ng",
    "e": "electron .",
    "start": "npm-run-all -p electron:serve ng:serve",
    "build": "npm run electron:serve-tsc && ng build --base-href ./",
    "build:dev": "npm run build -- -c dev",
    "build:prod": "npm run build -- -c production",
    "ng:serve": "ng serve -c production",
    "ng:serve:web": "ng serve -c web -o",
    "electron:start": "electron ./dist-electron/main.js",
    "electron:serve-tsc": "tsc -p tsconfig-serve.json",
    "electron:serve": "wait-on http-get://localhost:4200/ && npm run electron:serve-tsc && electron ./dist-electron/main.js --serve",
    "electron:local": "npm run electron:serve-tsc && electron ./dist-electron/main.js",
    "electron:linux": "npm run build:prod && electron-builder build --linux",
    "electron:windows": "npm run build:prod && electron-builder build --windows",
    "electron:mac": "npm run build:prod && electron-builder build --mac --dir",
    "test": "ng test",
    "e2e": "npm run build:prod && cross-env TS_NODE_PROJECT='e2e/tsconfig.e2e.json' mocha --timeout 300000 --require ts-node/register e2e/**/*.e2e.ts",
    "version": "conventional-changelog -i CHANGELOG.md -s -r 0 && git add CHANGELOG.md",
    "lint": "ng lint"
  },
  "dependencies": {
    "@angular-devkit/build-angular": "^12.2.5",
    "@angular/animations": "~12.2.5",
    "@angular/cdk": "^12.2.5",
    "@angular/common": "~12.2.5",
    "@angular/compiler": "~12.2.5",
    "@angular/compiler-cli": "^12.2.7",
    "@angular/core": "~12.2.5",
    "@angular/fire": "^5.4.2",
    "@angular/forms": "~12.2.5",
    "@angular/localize": "^12.2.5",
    "@angular/platform-browser": "~12.2.5",
    "@angular/platform-browser-dynamic": "~12.2.5",
    "@angular/router": "~12.2.5",
    "@auth0/angular-jwt": "^4.0.0",
    "@nestjs/common": "^6.11.8",
    "@nestjs/core": "^6.11.8",
    "bcryptjs": "^2.4.3",
    "bootstrap": "^4.4.1",
    "chart.js": "^2.9.3",
    "chartjs-plugin-annotation": "^0.5.7",
    "class-transformer": "^0.2.3",
    "config": "^3.3.1",
    "core-js": "^3.6.5",
    "file-saver": "^2.0.2",
    "font-awesome": "^4.7.0",
    "jsonwebtoken": "^8.5.1",
    "mosca": "^2.8.3",
    "mqtt": "^3.0.0",
    "nestjs": "0.0.1",
    "ngx-electron": "^2.2.0",
    "nodemailer": "^6.4.4",
    "primeicons": "^4.0.0",
    "primeng": "^9.1.3",
    "raw-loader": "^4.0.2",
    "reflect-metadata": "^0.1.13",
    "rxjs": "~6.5.4",
    "sequelize": "^5.21.5",
    "sequelize-typescript": "^1.1.0",
    "sqlite3": "^5.0.0",
    "xlsx": "^0.16.3",
    "zone.js": "^0.11.4"
  },
  "devDependencies": {
    "@angular/cli": "^12.2.6",
    "@types/chart.js": "^2.9.11",
    "@types/config": "0.0.36",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "@types/xml2js": "^0.4.5",
    "electron": "^9.0.5",
    "electron-builder": "^22.7.0",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.0",
    "karma-jasmine": "~2.0.1",
    "karma-jasmine-html-reporter": "^1.4.2",
    "npm-run-all": "^4.1.5",
    "protractor": "~5.4.3",
    "ts-node": "~8.3.0",
    "tslint": "~5.18.0",
    "typescript": "~4.3.5",
    "wait-on": "^4.0.0"
  }
}
